{
  "version": 2,
  "builds": [
    {
      "src": "api/index.jsx",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.jsx",
      "methods":["GET","POST","PUT","DELETE","PATCH","OPTIONS"],
      "headers":{
        "Access-Control-Allow-Origin":"*"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 
